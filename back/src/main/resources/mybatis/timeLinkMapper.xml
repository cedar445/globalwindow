<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxq.globalwindow.mapper.timeLinkMapper">
    <update id="setTimeLink">
        insert into timelink values (#{timeLink})
    </update>
    <select id="findAlltimeLink" resultType="com.zxq.globalwindow.pojo.timeLink">
        select * from timelink
    </select>
</mapper>